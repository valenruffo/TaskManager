#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/valen/Desktop/valen/projects/TaskManager/back/node_modules/mime/node_modules:/mnt/c/Users/valen/Desktop/valen/projects/TaskManager/back/node_modules:/mnt/c/Users/valen/Desktop/valen/projects/TaskManager/node_modules:/mnt/c/Users/valen/Desktop/valen/projects/node_modules:/mnt/c/Users/valen/Desktop/valen/node_modules:/mnt/c/Users/valen/Desktop/node_modules:/mnt/c/Users/valen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/valen/Desktop/valen/projects/TaskManager/back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/valen/Desktop/valen/projects/TaskManager/back/node_modules/mime/node_modules:/mnt/c/Users/valen/Desktop/valen/projects/TaskManager/back/node_modules:/mnt/c/Users/valen/Desktop/valen/projects/TaskManager/node_modules:/mnt/c/Users/valen/Desktop/valen/projects/node_modules:/mnt/c/Users/valen/Desktop/valen/node_modules:/mnt/c/Users/valen/Desktop/node_modules:/mnt/c/Users/valen/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/valen/Desktop/valen/projects/TaskManager/back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
